package com.aurionpro.mini.project.TicTacToeUsingFacade;

public class Players {
	String name;
	String symbol;
	
	public void players(String name , String symbol) {
		this.name=name;
		this.symbol=symbol;
		
	}
	
	public String getName() {
		return name;
	}
	
	public String getSymbol() {
		return symbol;
	}

}
