package com.weekly.assignment02.GuitarInventory;

public class GuitarSpec {
	private String builder;
	private String model;
	private String type;
	private String backWood;
	private String topWood;

	public GuitarSpec(String builder, String model, String type, String backWood, String topWood) {
		this.builder = builder;
		this.model = model;
		this.type = type;
		this.backWood = backWood;
		this.topWood = topWood;
	}

	public boolean matches(GuitarSpec comp) {
	    if (builder != null && !builder.equalsIgnoreCase(comp.backWood))
	        return false;
		return false;
		
		
		    if (model != null && !model.equalsIgnoreCase(comp.model))
		        return false;
			return false;
	}

}
